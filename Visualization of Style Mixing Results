# Display images: Original (z1) - Original (z2) - Result after style mixing
plt.figure(figsize=(15, 5))
titles = ['Style from A', 'Style from B', 'Style Mixed']

for i, img in enumerate([img1, img2, img_mixed]):
    plt.subplot(1, 3, i+1)
    plt.imshow(img)
    plt.axis('off')
    plt.title(titles[i])

plt.tight_layout()
plt.show()
